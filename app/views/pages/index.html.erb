<div class="row">
  <div class="col-3 text-center mx-auto mt-5">
    <%= link_to "Log in", new_admin_session_path, class: "btn btn-secondary" %>
  </div>

  <% if admin_signed_in? %>
    <div class="col-3 text-center mx-auto mt-5">
      <%= link_to "Dashboards", backoffice_root_path, class: "btn btn-primary" %>
    </div>

    <div class="col-3 text-center mx-auto mt-5">
      <%= link_to "Logout", destroy_admin_session_path, method: :delete, class: "btn btn-dark" %>
    </div>    
  <% end %>
</div>